<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zy.open.demo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".DemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".bundle.FromActivity" />
        <activity android:name=".bundle.ToActivity" />
        <activity android:name=".list.SingleTypeListActivity" />
        <activity android:name=".list.MultiTypeListActivity" />
        <activity android:name=".recycler.RecyclerEntranceActivity">
        </activity>
        <activity android:name=".recycler.single.SingleAdapterActivity" />
        <activity android:name=".recycler.multi.MultiAdapterActivity" />
        <activity android:name=".title.TestTitleActivity">
        </activity>
        <activity android:name=".exception.ExceptionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>